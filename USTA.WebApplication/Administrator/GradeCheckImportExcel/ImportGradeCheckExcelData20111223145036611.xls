	levent.attachOnCheckLoginFinish(function() {
		if (QZFL.cookie.get('unsubmail') == 1) {
			QZFL.cookie.del('unsubmail');
			unSubMail();
		}

		//已经登录才继续订阅流程
		if (userQQ != 0) {
			var subType = QZFL.cookie.get('qqmailsub');
			if (subType == 1) {
				subQQMail(subType);
			} else if (subType == 2) {
				jointSub();
			}
		} else {
			QZFL.cookie.del('qqmailsub')
		}
	});

	function coverSubMail(type) {
		getTEXT('mail_unsub', rootUrl + '/' + mcity + "/mail/unsub?r="  + Math.random(), null, function(data) {
			try {
				eval(data);
			} catch (err) {
				alert('服务忙，请稍候重试');
				return;
			}
			if (RET_COMMON.retcode == 0) {
				subQQMail(type);
			} else {
				showMailError("" + RET_COMMON.retcode, RET_COMMON.retdata.city);
			}
		}, null, "GET");
	}

	function setNote(data){
		if(data){
			if(data.i_ret==0){
				switch(data.data[0]){
					case 1:
						$('paipai_dingyue').innerHTML = '已订阅每日精选，<a href="javascript:void(0);" onclick="showDiv(\'v:unsub_paipai_confirm_div\');" >取消订阅</a>';
						break;
					case 0:
						$('paipai_dingyue').innerHTML = '还可以<a href="javascript:void(0);" onclick="return getBookingInfo(1, \'setPaipaiBooking\');" >订阅每日精选</a>，打开QQ就能收到';
						break;
					default:
						break;
				}
			}
		}
		return false;
	}

	function delPaipaiMail(){
		this.disabled="disabled";
		var token = TUAN.Util.Token.paipaiToken();
		MyjsLoader('http://service.paipai.com/cgi-bin/shoppingguide_modify?option=2&callback=setPaipaiBooking&aioSrc=7&key='+paipaikey+'&uin='+userQQ+'&g_tk='+token+'&_'+Math.random());
		return false;
	}

	function conformQuitPaipai(){
		getBookingInfo(2, 'setPaipaiBooking');
		hideDiv('v:unsub_paipai_confirm_div');
		showDiv('paipai_quit_div');
		$('paipai_dingyue').innerHTML = '还可以<a href="javascript:void(0);" onclick="return getBookingInfo(1, \'setPaipaiBooking\');" >订阅每日精选</a>，打开QQ就能收到';
	}

	function bookPaipai() {
		$('paipai_dingyue').innerHTML = '已订阅每日精选，<a href="javascript:void(0);" onclick="showDiv(\'v:unsub_paipai_confirm_div\');" >取消订阅</a>';
	}

