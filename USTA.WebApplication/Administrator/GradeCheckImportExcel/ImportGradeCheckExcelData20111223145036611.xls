		$e('#subscribe_city_list').bind('click', function(evt) {
			var el = QQVIP.event.getTarget(evt);
			while (el) {
				if ((el.tagName || '').toUpperCase() == 'A') {
					break;
				}
				el = el.parentNode;
			}
			if (el) {
				set_sub_city(el, el.href.substr(el.href.lastIndexOf('/') + 1));
			}
			QQVIP.event.cancelBubble(evt);
			QQVIP.event.preventDefault(evt);
			return false;
		});

		//订阅浮出层
		var isHide = true;
		$e('#subscribe_city_widget').bind('mouseover', function(evt){
			isHide = false;
            $e('#subscribe_city').addClass('current');
			$e('#subscribe_city_list').show();
		});
